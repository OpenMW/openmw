          <widget class="QLabel" name="unarmedFactorsStrengthLabel">
           <property name="text">
            <string>Factor Strength into Hand-to-Hand Combat</string>
           </property>
          </widget>
         </item>
         <item row="0" column="1">
          <widget class="QComboBox" name="unarmedFactorsStrengthComboBox">
